package hometasks.Hometask9;

public class Keyboard {
    private String type;
    private boolean gamer;

    public Keyboard(String type, boolean gamer) {
        this.type = type;
        this.gamer = gamer;
    }

    public String getType(String type) {
        return type;
    }

    public boolean isGamer(boolean gamer) {
        if (gamer == true){
        System.out.println("The keyboard is for games");}
        if (gamer == false){
            System.out.println("The keyboard is not for games");}
        return gamer;
    }
}

